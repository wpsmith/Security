{
  "name": "wpsmith/security",
  "description": "WordPress security stuff.",
  "keywords": [
    "wordpress",
    "security"
  ],
  "homepage": "https://github.com/wpsmith/Security",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Travis Smith",
      "email": "t@wpsmith.net",
      "homepage": "https://wpsmith.net/",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/wpsmith/Security/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "composer/installers": "~1.0",
    "wpsmith/singleton": "^1.0.0",
    "wpackagist-plugin/limit-login-attempts-reloaded": "*"
  },
  "extra": {
    "installer-paths": {
      "src/{$name}/": [
        "wpackagist-plugin/limit-login-attempts-reloaded",
        "wpackagist-plugin/force-strong-passwords"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "WPS\\WP\\": "src/"
    }
  }
}
